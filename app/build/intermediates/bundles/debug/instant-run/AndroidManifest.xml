            android:screenOrientation="landscape" />
        <activity
            android:name="com.yiyun.sijianguan.activity.Pic_res"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.yiyun.sijianguan.activity.Seal_Activity"
            android:screenOrientation="portrait" />
